rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

  match /users/{user} {
      allow write;
      allow read: if request.auth != null
    }

    match /contacts/{contact} {
      allow write;
    }

    match /subscribers/{subscriber} {
      allow write;
    }

    match /tests/{test} {
      allow read;
    }

  }

}